<?xml version="1.0"?>
<Container version="2">
  <Name>traefik-V2-whoami-example1</Name>
  <Repository>traefik/whoami</Repository>
  <Registry>https://hub.docker.com/r/traefik/whoami</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://github.com/urbanswelt/unraid-docker-template/issues</Support>
  <Project>https://github.com/urbanswelt/unraid-docker-template</Project>
  <Overview>Tiny Go webserver that prints os information and HTTP request to output&#xD;
This is a Sub Domain example with active lets encrypt for Traefik V2</Overview>
  <Category>Tools:</Category>
  <WebUI/>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/docker-library/docs/a6cc2c5f4bc6658168f2a0abbb0307acaefff80e/traefik/logo.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1614462751</DateInstalled>
  <DonateText>Buy me a coffee</DonateText>
  <DonateLink>https://ko-fi.com/urbanswelt</DonateLink>
  <Description>Tiny Go webserver that prints os information and HTTP request to output&#xD;
This is a Sub Domain example with active lets encrypt for Traefik V2</Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish/>
  </Networking>
  <Data/>
  <Environment/>
  <Labels>
    <Label>
      <Value>true</Value>
      <Name>traefik.enable</Name>
      <Mode/>
    </Label>
    <Label>
      <Value>Host(`whoami.example.com`)</Value>
      <Name>traefik.http.routers.whoami.rule</Name>
      <Mode/>
    </Label>
    <Label>
      <Value>websecure</Value>
      <Name>traefik.http.routers.whoami.entrypoints</Name>
      <Mode/>
    </Label>
    <Label>
      <Value>lets-encr</Value>
      <Name>traefik.http.routers.whoami.tls.certresolver</Name>
      <Mode/>
    </Label>
  </Labels>
  <Config Name="traefik.enable" Target="traefik.enable" Default="" Mode="" Description="Container Label: traefik.enable" Type="Label" Display="always" Required="true" Mask="false">true</Config>
  <Config Name="traefik.http.routers.whoami.rule" Target="traefik.http.routers.whoami.rule" Default="" Mode="" Description="Container Label: traefik.http.routers.whoami.rule" Type="Label" Display="always" Required="true" Mask="false">Host(`whoami.example.com`)</Config>
  <Config Name="traefik.http.routers.whoami.entrypoints" Target="traefik.http.routers.whoami.entrypoints" Default="" Mode="" Description="Container Label: traefik.http.routers.whoami.entrypoints" Type="Label" Display="always" Required="true" Mask="false">websecure</Config>
  <Config Name="traefik.http.routers.whoami.tls.certresolver" Target="traefik.http.routers.whoami.tls.certresolver" Default="" Mode="" Description="Container Label: traefik.http.routers.whoami.tls.certresolver" Type="Label" Display="always" Required="true" Mask="false">lets-encr</Config>
</Container>
